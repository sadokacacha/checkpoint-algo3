
PROCEDURE dot_product (v1,v2)
VAR
    
BEGIN
    scalar_product=v1[0]*v2[0]+v1[1]*v2[1]
    write(scalar_product)
    IF (scalar_product==0) THEN
        write("the 2 vectors are orthogonal")
    else ("the 2 vectors are not orthogonal")
    END_IF

END
ALGORITHM algorithm_name
VAR
    v1,v2 : ARRAY_OF type[n];
    i :INTEGER
BEGIN
    write('')
FOR i FROM 1 TO n   DO
    dot_product(v1,v2)


END_FOR
    
END